{
"version": 2,
"builds":[
{
"src": "AASYSTEM/wsgi.py",
"use": "@vercel/python",
"config": { "maxLambdaSize": "15mb", "runtime": "python3.6.8" }
},
{
"src": "build_files.sh",
"use": "@vercel/static-build",
"config": {
"distDir":"staticfiles-build"
}
}
], 
"routes": [
{
"src": "/static/(.*)",
"dest": "/static/$1"
},
{
"src": "/(.*)",
"dest": "AASYSTEM/wsgi.py"
}
]
}